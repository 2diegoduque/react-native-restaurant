{
  "expo": {
    "name": "Restaurantes",
    "slug": "restaurantes",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00a680"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.diegoduque.restaurants",
      "supportsTablet": true
    },
    "android": {
      "package": "com.diegoduque.restaurants",
      "permissions": ["CAMERA_ROLL"]
    }
  }
}
